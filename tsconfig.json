{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"],
      "@components/*": ["src/components/*"],
      "@utils/*": ["src/utils/*"],
      "@pages/*": ["src/pages/*"],
      "@libs/*": ["src/libs/*"],
      "@types/*": ["src/types/*"],
      "@theme/*": ["src/theme/*"],
      "@store/*": ["src/store/*"],
      "@data/*": ["src/data/*"],
      "@context/*": ["src/context/*"],
      "@layouts/*": ["src/layouts/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "target": "ES2015",
    "lib": ["ES2019", "DOM", "DOM.Iterable"],
    "module": "ESNext",
    "strictNullChecks": true
  }
}